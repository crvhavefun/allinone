  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="dw_plu">
          <DataField>dw_plu</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="dw_longdesc">
          <DataField>dw_longdesc</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="dw_amt">
          <DataField>dw_amt</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="dw_qty">
          <DataField>dw_qty</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Amt_rank">
          <DataField>Amt_rank</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="Qty_rank">
          <DataField>Qty_rank</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select   s.dw_plu, s.dw_longdesc, s.dw_amt, s.dw_qty,
         RANK() over (order by s.dw_amt desc) as Amt_rank,
         RANK() over (order by s.dw_qty desc) as Qty_rank

from (
     select	m.dw_plu, m.dw_longdesc, sum(i.dw_amt) dw_amt, SUM(i.dw_qty) dw_qty

     from	dw_itemdsd i, dw_itemmas m

     where	i.dw_bu = m.dw_bu and i.dw_plu = m.dw_plu 
	and i.dw_bu = @bu and m.dw_bu = @bu
	and i.dw_txdate between @sdate and @edate
	and i.dw_amt &lt;&gt; 0 and i.dw_storecode = @shop

     group by m.dw_plu, m.dw_longdesc
) s

order by 5</CommandText>
        <QueryParameters>
          <QueryParameter Name="@bu">
            <Value>=Parameters!bu.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@sdate">
            <Value>=Parameters!sdate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@edate">
            <Value>=Parameters!edate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@shop">
            <Value>=Parameters!shop.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
