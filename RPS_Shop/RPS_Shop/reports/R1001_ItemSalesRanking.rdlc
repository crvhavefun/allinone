        </Field>
        <Field Name="Amt_rank">
          <DataField>Amt_rank</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="Qty_rank">
          <DataField>Qty_rank</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select   s.dw_plu, s.dw_longdesc, s.dw_group3, s.dw_group3desc, s.dw_buyer, 
	s.dw_amt, s.dw_qty, s.gp,
	RANK() over (order by s.dw_amt desc) as Amt_rank,
	RANK() over (order by s.dw_qty desc) as Qty_rank

from (
     select	m.dw_plu, m.dw_longdesc, m.dw_group3, m.dw_group3desc, m.dw_buyer, 
	sum(i.dw_amt) dw_amt, SUM(i.dw_qty) dw_qty, SUM(i.dw_amt-i.dw_cost) gp

     from	dw_itemdsd i, dw_itemmas m

     where	i.dw_bu = m.dw_bu and i.dw_plu = m.dw_plu 
	and i.dw_bu = 'HBA' and m.dw_bu = 'HBA'
	and i.dw_txdate between '2011-01-01' and '2011-01-01'
	and i.dw_storecode between '099301' and '099301'
	and i.dw_amt &lt;&gt; 0 

     group by m.dw_plu, m.dw_longdesc, m.dw_group3, m.dw_group3desc, m.dw_buyer
) s</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
