        </Field>
        <Field Name="dw_amt">
          <DataField>dw_amt</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select dw_plu, dw_longdesc, dw_txdate, sum(dw_amt) dw_amt
from dw_itemds
where dw_bu = 'CRV' and dw_txdate between '2010-09-01' and '2010-09-05'
and dw_group6 = '0' and dw_isfreshgoods = '0'
group by dw_plu, dw_longdesc, dw_txdate
order by dw_plu, dw_longdesc, dw_txdate</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
