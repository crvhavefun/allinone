      <Fields>
        <Field Name="dw_plu">
          <DataField>dw_plu</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="dw_longdesc">
          <DataField>dw_longdesc</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="dw_txdate">
          <DataField>dw_txdate</DataField>
