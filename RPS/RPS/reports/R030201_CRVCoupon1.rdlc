      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select 	b.dw_txdate, b.dw_txtime, b.dw_storecode, b.dw_tillid, b.dw_docno, b.dw_payamount, 
		b.dw_tendercode, substring(b.dw_extendparam,1,14) dw_extendparam
from 	dw_transsalestender b 
where 	b.dw_txdate between '2010-11-01' and '2010-11-30' and 
		b.dw_extendparam like '%2011083005078%'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
